<h1>Sessions#new</h1>

<p id="alert"><%= alert %></p>
<h1>Login</h1>
<%= form_tag sessions_path do |form| %>
  <div class="field">
    <%= label_tag :cpf %>
    <%= text_field_tag :cpf %>
  </div>
  <div class="field">
    <%= label_tag :senha %>
    <%= password_field_tag :senha %>
  </div>
  <div class="actions">
    <%= submit_tag "Login" %>
  </div>
<% end %>
